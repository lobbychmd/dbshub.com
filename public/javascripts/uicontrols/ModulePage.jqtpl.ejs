
<style >
    .ModulePage {
        background: none repeat scroll 0 0 #D8E8FF;
        padding: 10px;
    }
    /* 标签页(基于 jquery ui) */
.ModuleTabs.ui-tabs{border-radius:0px; border: 0px; padding: 0px;}
.ModuleTabs.ui-tabs>ul { border-width: 0px; padding: 0 0 0 6px;   }
.ModuleTabs.ui-tabs>.ui-widget-header {background: url("/javascripts/uicontrols/images/header_bg.png") repeat scroll 0 0 transparent;border-top-left-radius:0px;border-bottom-right-radius:0px;border-top-right-radius:0px;border-bottom-left-radius:0px;  }
.ModuleTabs.ui-tabs>.ui-tabs-nav{height:32px;}
.ModuleTabs>ul>li .ui-icon-close {cursor: pointer; float: left; margin: 0.4em 0.6em 0 0;}

.ModuleTabs.ui-tabs>ul>li.ui-state-default {
    background:url("/") repeat-x scroll 50% 50%   ; border: 0px;    height: 31px;  }
.ModuleTabs.ui-tabs>ul>li.ui-state-active{ background:  #CEE7FF; border-radius: 0px; height: 30px; border-width: 0 1px; border-color: #7791AC; border-style: solid; }
.ModuleTabs.ui-tabs>.ui-tabs-panel {background:  #CEE7FF}

</style>

<script type='text/x-jquery-tmpl' id="uitpModulePage">
    <div class="ModulePage" module="${pageInfo.ModuleID}" page="${pageInfo.PageID}">
        <span id="ActiveFlowStr" style="display:none">${ActiveFlowStr}</span>
    </div>
</script>


<script type='text/x-jquery-tmpl' id="tpEditor">
    {{if metaField.EditorType=="LIST" }}
        <select name="${name}" id="${id}" >
            {{each metaField.Selection}}
            <option value="${key}" selected="selected">${value}</option>
            {{/each}}
            <option value="" selected="selected"></option>
        </select>
    {{else}}
        <input value="${value}" name="${name}" id="${id}" fn ="${name}" 
            class="editor {{if metaField.EditorType=="DATE" }}DateEditor{{/if}}" {{if metaField.EditorType=="DATE" }}type="date"{{/if}}
        />
    {{/if}}
    
</script>


<script type='text/x-jquery-tmpl' id="uitpModuleTabs">
    <div id="ModuleTabs" align="auto" layout="v" class="ModuleTabs">
        <ul align="top">
        </ul>
    </div>
</script>
